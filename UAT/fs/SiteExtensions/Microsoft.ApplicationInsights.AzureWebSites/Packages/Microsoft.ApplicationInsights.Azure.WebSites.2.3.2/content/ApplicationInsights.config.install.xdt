<ApplicationInsights xmlns="http://schemas.microsoft.com/ApplicationInsights/2013/Settings" xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <TelemetryModules xdt:Transform="InsertIfMissing">
    <Add xdt:Transform="InsertIfMissing" xdt:Locator="Match(Type)" Type="Microsoft.ApplicationInsights.Extensibility.AzureWebSites.DiagnosticsTelemetryModule, Microsoft.ApplicationInsights.AzureWebSites"/>
  </TelemetryModules>
  <TelemetryInitializers xdt:Transform="InsertIfMissing"/>
  <TelemetryInitializers>
    <Add xdt:Transform="InsertIfMissing" xdt:Locator="Match(Type)" Type="Microsoft.ApplicationInsights.Extensibility.AzureWebSites.SdkSourceTelemetryInitializer, Microsoft.ApplicationInsights.AzureWebSites"/>
  </TelemetryInitializers>
  <AzureWebSiteExtension xdt:Transform="InsertIfMissing"/>
</ApplicationInsights>